set_aws_default_profile() {
	if [ $# -eq 1 ]; then
		newsettings=$(php ~/bin/aws_profile_scripts/AWSCredentials.php set-profile $1)
		eval $newsettings
	else
		echo "Possible profiles to use:"
		php ~/bin/aws_profile_scripts/AWSCredentials.php list-profiles
	fi
}

alias aws-profile=set_aws_default_profile
alias aws-rotate-access-keys='php ~/bin/aws_profile_scripts/AWSCredentials.php rotate-all-keys'
